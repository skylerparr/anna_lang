defmodule Foo.Bar.Sample do

  deftype Turtle do
    {:color, String}
    {:age, Int}
    {:name, String}
  end

  @spec(sum, {Int, Int}, Int)
  def sum(a, b) do
    a + b
  end

  @spec(hello_world, null, String)
  def hello_world() do
    "hello world"
  end

  # Types can be inferred too
  def test() do
    "works"
  end
end