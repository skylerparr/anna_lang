defmodule Foo.Bar.Sample do

  @spec(sum, {Int, Int}, Int)
  def sum(a, b) do
    a + b
  end

  @spec(hello_world, nil, String)
  def hello_world() do
    "hello world"
  end

  # Types can be inferred too
  def test() do
    "works"
  end

  @spec(get_a_turtle, nil, Foo.Bar.Turtle)
  def get_a_turtle() do
    %{"name" => "bae", "age" => 25, "type" => "green"}
  end

end