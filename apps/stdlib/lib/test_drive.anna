defmodule(TestDrive, {

  @def test([String], {
    str = Str.random(10);
    @native IO.inspect('I made a simple random string');
    @native IO.inspect(str);
    result = AppCode.get_api(@_'Kernel');
    @native IO.inspect(result);

    self = Kernel.self();
    @native IO.inspect(self);
    pid = Kernel.start(@fn {
      [{}] => {
        @native IO.inspect("side job");
        Kernel.send(self, @_'success');
      };
    });
  });

});